{
  "author": "<aperos.dev@gmail.com>",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@aperos/event-emitter": "*",
    "@aperos/rpc-common": "*"
  },
  "description": "TypeScript implementation of a RPC web client based on WebSockets",
  "devDependencies": {
    "dts-bundle": "latest",
    "make-dts-bundle": "latest",
    "parcel": "latest",
    "rollup": "latest",
    "rollup-plugin-terser": "latest",
    "rollup-plugin-typescript2": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "license": "MIT",
  "name": "@aperos/rpc-web-client",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c && npm run build-dts",
    "build-dts": "make-dts-bundle",
    "build-production": "BUILD=production npm run build",
    "clean": "rm -rf .cache build dist",
    "prepare": "npm run clean && npm run build-production",
    "test-client": "parcel serve examples/test_client/index.html"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.6"
